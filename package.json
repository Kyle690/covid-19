    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.12",
    "node-schedule": "^1.3.2",
    "nodemon": "^2.0.3",
    "save": "^2.4.0"
  }
